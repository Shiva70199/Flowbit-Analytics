{
  "name": "flowbit-analytics-monorepo",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/web",
    "apps/api",
    "services/vanna"
  ],
  "scripts": {
    "dev:web": "npm run dev:web --workspace=apps/web",
    "dev:api": "npm run local:dev --workspace=apps/api",
    "start:vanna": "npm run start:vanna --workspace=services/vanna",
    "dev": "npm-run-all -p dev:web dev:api start:vanna",
    "db:seed": "ts-node --esm apps/api/prisma/seed.ts"
  },
  "dependencies": {
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "next": "^16.0.1",
    "npm-run-all": "^4.1.5",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@prisma/client": "^6.19.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.0",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "nodemon": "^3.1.10",
    "prisma": "^6.19.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
